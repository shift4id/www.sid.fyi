{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": { "order": "asc" },
        "groups": [["builtin", "external"], "internal", ["parent", "sibling", "index"]],
        "newlines-between": "always",
        "pathGroups": [{ "pattern": "@/**", "group": "parent" }]
      }
    ]
  }
}
